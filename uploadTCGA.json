{
    "pipeline": {
      "name": "uploadTCGA"
    },
    "transform": {
      "image": "bhklab/python_zenodo",
      "cmd": [
            "python3",
            "/pfs/ZenodoUpload/upload_multi.py"
        ]
    },
    "resource_requests": {
      "memory": "8G"
    },
    "input": {
      "cross": [
        {
          "git": {
            "url": "https://github.com/BHKLAB-Pachyderm/ZenodoUpload.git",
            "name": "",
            "branch": "master"
          }
        },
        {
            "pfs": {
              "repo": "downloadTCGA",
              "name": "input",
              "glob": "/*.rds"
            }
        },
        {
            "pfs": {
              "repo": "downloadTCGA",
              "name": "input",
              "glob": "/*.csv"
            }
        }
      ]
    }
}